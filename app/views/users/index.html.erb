.rotate {
  animation: rotation 8s infinite linear;
}

@keyframes rotation {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(359deg);
  }
}

.pulse {
  animation: pulse 3s infinite;
  margin: 0 auto;
  display: table;
  /* margin-top: 50px; */
  animation-direction: alternate;
  -webkit-animation-name: pulse;
  animation-name: pulse;
}

@-webkit-keyframes pulse {
  0% {
    -webkit-transform: scale(1);
  }
  50% {
    -webkit-transform: scale(1.1);
  }
  100% {
    -webkit-transform: scale(1);
  }
}

@keyframes pulse {
  0% {
    transform: scale(1);
  }
  50% {
    transform: scale(1.1);
  }
  100% {
    transform: scale(1);
  }
}

.layer--particles {
  -webkit-mask-image: url(<%= asset_path "tokyo_final.jpg"%>);
          mask-image: url(<%= asset_path "tokyo_final.jpg"%>);
  mask-mode: luminance;
  -webkit-mask-size: 100% 100%;
          mask-size: 100% 100%;
          animation-duration: 4s;
}
.layer--particles::after{
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  width: 20%;
  height: 100%;
  background: url(<%= asset_path "ember-particles.png"%>) 0% 0% repeat;
  background-size: 50%;
  animation-duration: 4s;
  -webkit-animation: particle-rise 15s linear infinite;
          animation: particle-rise 15s linear infinite;
}

@-webkit-keyframes particle-rise {
  0% {
    -webkit-transform: rotate(0deg) translateY(0%);
            transform: rotate(0deg) translateY(0%);
  }
  100% {
    -webkit-transform: rotate(0deg) translateY(-50%);
            transform: rotate(0deg) translateY(-50%);
  }
}

@keyframes particle-rise {
  0% {
    -webkit-transform: rotate(0deg) translateY(0%);
            transform: rotate(0deg) translateY(0%);
  }
  100% {
    -webkit-transform: rotate(0deg) translateY(-50%);
            transform: rotate(0deg) translateY(-50%);
  }
}

@keyframes moon {
	from {
		transform: rotate(0deg)
		           translate(-80px)
		           rotate(0deg);
	}
	to {
		transform: rotate(360deg)
		           translate(-80px)
		           rotate(-360deg);
	}
}

.moon {
  -webkit-mask-image: url(<%= asset_path "voyager_final.jpg"%>);
